    "@types/node": "^12.12.11",
    "@types/supertest": "^2.0.8",
    "jest": "^24.9.0",
    "node-notifier": ">=8.0.1",
    "nodemon": "^2.0.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.5.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.1",
    "typescript": "^3.7.2"
  },
